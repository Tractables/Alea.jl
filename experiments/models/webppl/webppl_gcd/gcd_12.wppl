var gcd = function(a, b) {
    var cond = (b == 0);
    var answer = cond ? a:gcd(b, (a % b));

    return answer;
}

var main = function() {
    var a = randomInteger(4096) + 1;
    var b = randomInteger(4096) + 1;
    var answer = (gcd(a, b) == 1);
    return answer;
}
timeit(function(){Infer({method:'enumerate',model:main}).getDist('true')}).runtimeInMilliseconds;
