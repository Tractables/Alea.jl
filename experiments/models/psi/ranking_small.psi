def update(ps:Z[], dp:Z[]) {
    ns := array(4, 0:Z);
    for i in [0..4) {
        ns[i] = ps[i] + uniformInt(-4, 4);
    }
    for i in [0..3) {
        observe(ns[dp[i]] < ns[dp[i+1]]);
    }
}

def main(){
    ps := array(4, 0:Z);
    
    for i in [0..4) {
        ps[i] = uniformInt(0, 3);
    }
    for i in [0..4) {
        for j in [i+1..4) {
            observe(ps[i] != ps[j]);
        }
    }

    data:Z[][] := [[3, 2, 1, 0]];
    update(ps, data[0]);
    return ps;
}
