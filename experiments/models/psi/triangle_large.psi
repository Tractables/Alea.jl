def check(a, b, c) {
    if ((a <= 0) || (b <= 0) || (c <= 0)) {
      return 3;
    }
    else if ((a+b<=c) || (a+c<=b) || (b+c <= a)) {
        return 3;
    }
    else if ((a == b) && (b == c) && (a == c)) {
        return 2;
    }
    else if ((a == b) || (b == c) || (a == c)) {
        return 1;
    }
    else {
        return 0;
    }
}


def main(){
    a := uniformInt(-500, 500);
    b := uniformInt(-500, 500);
    c := uniformInt(-500, 500);

    return check(a, b, c);
}
