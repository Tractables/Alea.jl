        
def main(){
    ps := array(4, 0:Z);
    
    for i in [0..ps.length) {
        ps[i] = uniformInt(0, 3);
    }
    for i in [0..ps.length) {
        for j in [i+1..ps.length) {
            observe(ps[i] != ps[j]);
        }
    }
    
    data := [[0, 2], [0, 3]];

    for i in [0..data.length) {
        observe(ps[data[i][0]] < ps[data[i][1]]);
    }
    
    return ps;
}