def main(){
    a := uniformInt(1, 1024);
    b := uniformInt(1, 1024);
repeat 16 
    {
        converged := (b == 0);
        amb := (a % b);
        if converged {
            a = a;
            b = b;
        }
        else {
            a = b;
            b = amb;
        }
    }
    answer := (a == 1);
    return answer;
}
